<!-- 搜索框 -->



<view wx:for="{{shareList}}" wx:for-item='item' wx:key='key'>
  <view class='collect-box' bindtap='goDetail' data-sid="{{item.sid}}">
    <view class='news'>
      <image class='newsIcon' src='{{item.simg1}}' />
      <!-- 图片选取图片列表的一张图片就行第一张图片 -->
      <view class='ContentV'>
        <view style='width:100%;height:80rpx;'>
          <text class='stitle'>新闻标题{{item.stitle}}</text>
        </view>
        <view style="position:relative;bottom:10rpx;">
          <text class='authorName'>{{item.user.uname}}</text>
          <text class='sdate'>{{item.sdate}}</text>
        </view>
      </view>
      <!-- <view class='ContentV'> end -->
      <image class='delIcon' src="../../images/delete2.png" catchtap='delShare' data-sid="{{item.sid}}" />
    </view>
    
    
    <!-- <view class='news'> end -->
  </view>

</view>